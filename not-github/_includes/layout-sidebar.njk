{% extends 'layout-default.njk' %}

{% block themeBody %}
  <header id="main-header" class="header-grid-area">
    {% block header %}
      {% include 'partials/header.njk' %}
    {% endblock header%}
  </header>

  <div class="site-banner rocket-content ga-head2-main">
    <p>[banner goes here, specific to this site. (not adsense, for example)]</p>
  </div>

  <div class="rocket-content ga-head3-main">
    <p>head3-main</p>
  </div>

  <div class="rocket-content ga-head4-main">
    <p>head4-main</p>
  </div>

  <div class="ga-content-l"></div>
  <div class="sidebar-grid-area">
    {% block sidebar %}
      {% include 'partials/_layoutSidebar/sidebar.njk' %}
    {% endblock sidebar %}
  </div>
  <div class="content-grid-area">
    {% block content %}
      {% include 'partials/_layoutSidebar/content.njk' %}
    {% endblock content %}
  </div>
  <div class="ga-content-r"></div>

  <div class="ga-foot-main">
    {% block footer %}
      {% include 'partials/footer.njk' %}
    {% endblock footer %}
  </div>

  <div class="ga-content-r"></div>


  {% include 'partials/bottom.njk' %}
{% endblock themeBody %}
